{
  "name": "omnigraph",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "test": "vitest --config infra/config/vitest.config.ts",
    "test:ui": "vitest --ui --config infra/config/vitest.config.ts",
    "test:run": "vitest run --config infra/config/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config infra/config/vitest.config.ts",
    "test:integration": "vitest run --config infra/config/vitest.config.ts src/test/integration",
    "test:containers": "vitest run --config infra/config/vitest.config.ts src/test/integration --reporter=verbose && pnpm docker:cleanup",
    "docker:test:up": "docker compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker compose -f docker-compose.test.yml down -v",
    "docker:cleanup": "bash scripts/cleanup-containers.sh",
    "lint": "eslint src --fix --config infra/config/eslint.config.mjs",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "3d-force-graph": "^1.78.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2",
    "inversify": "^6.2.2",
    "lit": "^3.3.1",
    "mitt": "^3.0.1",
    "nanostores": "^1.0.1",
    "neo4j-driver": "^5.28.1",
    "reflect-metadata": "^0.2.2",
    "three": "^0.177.0",
    "tree-sitter": "^0.22.4",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-python": "^0.23.6",
    "tree-sitter-typescript": "^0.23.2",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "~20.19.9",
    "@types/three": "^0.177.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/ui": "^2.1.9",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "testcontainers": "^11.4.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.9"
  }
}
